{
  "name": "stonk-cli",
  "description": "cli for querying all the stonks",
  "version": "0.0.2",
  "author": "Binil Pokhrel @binilpokhrel",
  "bin": {
    "stonk-cli": "./bin/run"
  },
  "bugs": "https://github.com/binilpokhrel/stonk-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.5.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "mysql2": "^2.2.5",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/binilpokhrel/stonk-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stonk-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "comment": {
        "description": "manage comments about a stock articles, companies, or trade histories"
      },
      "ipo": {
        "description": "manage ipo data related to a particular stock"
      },
      "news": {
        "description": "manage news articles related to particular stocks"
      },
      "price": {
        "description": "manage price information of one or more stocks, on a single date, single quarter, or range of either"
      }
    },
    "macos": {
      "identifier": "com.stonks.cli"
    }
  },
  "repository": "binilpokhrel/stonk-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
